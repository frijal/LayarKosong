{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    ":combinePatchMinorReleases",
    ":ignoreUnstable"
  ],
  "onboarding": false,
  "requireConfig": "optional",
  "dependencyDashboard": true,
  "timezone": "Asia/Makassar",
  "schedule": ["at any time"],
  "prConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "enabledManagers": [
    "npm",
    "github-actions",
    "pip_setup",
    "pip_requirements",
    "dockerfile",
    "regex"
  ],
  "customManagers": [
    {
      "customType": "regex",
      "managerFilePatterns": [
        "**/*.html",
        "**/*.json"
      ],
      "description": "Scanning seluruh file HTML dan JSON untuk mencari library CDN",
      "matchStrings": [
  "^https:\\/\\/unpkg\\.com\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/cdn\\.jsdelivr\\.net\\/npm\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/cdn\\.jsdelivr\\.net\\/gh\\/(?<depName>[^\\/\\s\"']+\\/[^\\"']+?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/cdnjs\\.cloudflare\\.com\\/ajax\\/libs\\/(?<depName>[^\\/\\s\"']+)\\/(?<currentValue>[^\\/\\s\"']+)",
  "^https:\\/\\/ajax\\.googleapis\\.com\\/ajax\\/libs\\/(?<depName>[^\\/\\s\"']+)\\/(?<currentValue>[^\\/\\s\"']+)",
  "^https:\\/\\/raw\\.githubusercontent\\.com\\/(?<depName>[^\\/\\s\"']+\\/[^\\"']+?)\\/(?<currentValue>[^\\/\\s\"']+)\\/",
  "^https:\\/\\/raw\\.githack\\.com\\/(?<depName>[^\\/\\s\"']+\\/[^\\"']+?)\\/(?<currentValue>[^\\/\\s\"']+)\\/",
  "^https:\\/\\/cdn\\.skypack\\.dev\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/esm\\.sh\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/jspm\\.io\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/ga\\.jspm\\.io\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/cdn\\.pika\\.dev\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?",
  "^https:\\/\\/cdn\\.rawgit\\.com\\/(?<depName>[^\\/\\s\"']+\\/[^\\"']+?)\\/(?<currentValue>[^\\/\\s\"']+)\\/",
  "^https:\\/\\/unpkg\\.com\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:\\?.*version=(?<currentValue>[^&\\s\"']+))?",
  "^https:\\/\\/cdn\\.jsdelivr\\.net\\/combine\\/.+",
  "^https:\\/\\/cdn\\.jsdelivr\\.net\\/npm\\/(?<depName>@?[^\\/\\s\"']+(?:\\/[^\\/\\s\"']+)?)(?:@(?<currentValue>[^\\/\\s\"']+))?\\/.+",
  "^https:\\/\\/fonts\\.googleapis\\.com\\/css\\?.*family=(?<depName>[^&\\s\"']+)",
  "^https:\\/\\/fonts\\.gstatic\\.com\\/(?<depName>[^\\/\\s\"']+)\\/(?<currentValue>[^\\/\\s\"']+)"
],
      "datasourceTemplate": "npm",
      "versioningTemplate": "semver"
    }
  ],
  "packageRules": [
    {
      "matchDepTypes": [
        "dependencies",
        "devDependencies"
      ],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "digest"
      ],
      "automerge": true,
      "platformAutomerge": true,
      "groupName": "all-minor-patch-auto"
    }
  ],
  "ignorePaths": [
    "**/node_modules/**",
    "**/.git/**",
    "**/img/**",
    "**/ext/icons/**",
    "**/ext/fontawesome-webfonts/**"
  ],
  "repositoryCache": "enabled"
}
