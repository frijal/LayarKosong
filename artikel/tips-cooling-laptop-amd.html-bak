<script>
    const imgs = document.querySelectorAll('.container img');
    const lightbox = document.getElementById('lightbox');
    const lbImg = lightbox.querySelector('img');
    let current = 0;
    function showLightbox(i){
        lbImg.src = imgs[i].src;
        lbImg.alt = imgs[i].alt; // Set alt text for accessibility
        lightbox.style.display = 'flex';
        current = i;
    }
    // Attach click event to all images with class 'cooling-image'
    imgs.forEach((img, i) => {
        img.addEventListener('click', () => showLightbox(i));
    });
    document.getElementById('closeBtn').onclick = () => lightbox.style.display = 'none';
    document.getElementById('prevBtn').onclick = () => {
        current = (current - 1 + imgs.length) % imgs.length;
        lbImg.src = imgs[current].src;
        lbImg.alt = imgs[current].alt;
    };
    document.getElementById('nextBtn').onclick = () => {
        current = (current + 1) % imgs.length;
        lbImg.src = imgs[current].src;
        lbImg.alt = imgs[current].alt;
    };
</script>
